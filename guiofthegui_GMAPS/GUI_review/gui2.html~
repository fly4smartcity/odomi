				<div class="col-sm-4" ><!-- cam -->
					<span id='cam'></span>
					<script>
						var n=0;
						var height='';
						var width='100%';
						var cam1="<img src='http://localhost:8080/stream?topic=/camera/image_raw' height='"+height+"' width='"+width+"'></img>";
						var cam1="<img src='http://localhost:8080/stream?topic=/camera/image_raw2' height='"+height+"' width='"+width+"'  style = 'position:relative;'></img>";
						var print1="<img src='http://localhost:8080/stream?topic=/camera/image_raw' height='"+height+"' width='"+width+"'></img><br></	div><img src='http://localhost:8080/stream?topic=/camera/image_raw2' height='"+height+"' width='"+width+"'  style = 	'position:relative;'></img>";
						var print2="<img src='http://localhost:8080/stream?topic=/camera/image_raw2' height='"+height+"' width='"+width+"'></img><br><img src='http://localhost:8080/stream?topic=/camera/image_raw' height='"+height+"' width='"+width+"'  style = 'position:relative; 	padding-top:0px;'></img>";
						document.getElementById('cam').innerHTML = print2;
						function change(){
						if(n%2==0){
							document.getElementById('cam').innerHTML = print1;
						}
						else
						{
							document.getElementById('cam').innerHTML = print2;
						}
						n++;
						}
					</script>
					<br><input type="button" value="Change_cam" onclick="change();">
