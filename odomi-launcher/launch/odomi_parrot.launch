<launch>

  <node pkg="drone_parrot" name="drone_parrot" type="drone_parrot" >
  	<param name="latitude" type="double" value="45.066144" />
  	<param name="longitude" type="double" value="7.661209" />
  	<param name="altitude" type="double" value="15.0" />
  </node>

 <node pkg="mission_planner" name="mission_planner" type="mission_planner">
 </node>

 <node pkg="odomi_path_planner" name="odomi_path_planner" type="odomi_path_planner">
<param name="bounding_box_extension" type="double" value="200.0" />
 </node>

 <node pkg="open_data_driver" name="open_data_driver" type="open_data_driver">
 </node>

 <node pkg="odomiadapter" name="odomiAdapter" type="odomiAdapter">
  	<param name="bounding_box_extension" type="double" value="200.0" />
 </node>

 <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch" />
</launch>
