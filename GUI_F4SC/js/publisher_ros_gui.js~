alert("enter in .js");
// Connecting to ROS
  // -----------------  
var ros_pub_msg = new ROSLIB.Ros({
    url : 'ws://localhost:9090'
  });

function contact_center(num,str)
{
	//alert("centro contattato");
  // Publishing a Topic
  // ------------------

  var contact = new ROSLIB.Topic({
    ros : ros_pub_msg,
    name : '/gui_ros_msg',
    messageType : 'gui_msg/msg_gui_ros'
  });

  var mex = new ROSLIB.Message({
    codice : parseInt(num)
    ,
    messaggio : str
  });
  contact.publish(mex);
}
